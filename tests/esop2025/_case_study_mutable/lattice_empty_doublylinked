ident emptydl;

annotation TopEmptyDoublyLinked() any :<==> "true" for "true";
annotation BottomEmptyDoublyLinked() any :<==> "false" for "true";

annotation PossiblyEmptyDoublyLinked() case_study.DoublyLinkedSortedList
    :<==> "§subject§ != null && ((§subject§.size == 0 && §subject§.first == null && §subject§.last == null) || (§subject§.first != null && §subject§.last != null))"
    for "true";

annotation NonEmptyDoublyLinked() case_study.DoublyLinkedSortedList
    :<==> "§subject§ != null && §subject§.size != 0 && §subject§.first != null && §subject§.last != null"
    for "true";

relation PossiblyEmptyDoublyLinked() <: TopEmptyDoublyLinked() :<==> "true";
relation NonEmptyDoublyLinked() <: TopEmptyDoublyLinked() :<==> "true";
relation NonEmptyDoublyLinked() <: PossiblyEmptyDoublyLinked() :<==> "true";
relation BottomEmptyDoublyLinked() <: NonEmptyDoublyLinked() :<==> "true";
relation BottomEmptyDoublyLinked() <: PossiblyEmptyDoublyLinked() :<==> "true";
relation BottomEmptyDoublyLinked() <: TopEmptyDoublyLinked() :<==> "true";
